<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<fx:root xmlns:fx="http://javafx.com/fxml/1" prefHeight="300.0" prefWidth="800.0"
  type="javafx.scene.layout.AnchorPane" xmlns="http://javafx.com/javafx/8"
  fx:controller="tech.subluminal.client.presentation.controller.LobbyUserController"
  fx:id="lobbyHost">
  <children>
    <VBox AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
      AnchorPane.topAnchor="0.0"
      alignment="CENTER" prefHeight="600.0" prefWidth="800.0">
      <children>
        <HBox alignment="CENTER" prefHeight="481.0" prefWidth="800.0">
          <children>
            <ListView style="-fx-background-color: black;" prefHeight="250.0" prefWidth="846.0"
              fx:id="userList"/>
          </children>
        </HBox>
        <Button mnemonicParsing="false" onAction="#onLobbyLeave" text="Leave Lobby"/>
        <Button mnemonicParsing="false" onAction="#onGameStart" text="Start Game"/>
      </children>
    </VBox>
  </children>
</fx:root>
